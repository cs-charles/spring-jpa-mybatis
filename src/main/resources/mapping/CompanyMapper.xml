<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.orange.spring.jpabatis.mapper.CompanyMapper">

    <select id="getCompanyList" resultType="com.orange.spring.jpabatis.domain.model.company.CompanyEntity">
        SELECT
            id,
            company_name,
            company_address
        FROM
            company
    </select>
</mapper>